<tr>
  <td><%= product.id %></td>
  <td><%= product.name %></td>
  <td><%= product.price %></td>
  <td><%= product.quantity %></td>
  <td><%= product.category.name %></td>
  <td>
    <%= link_to (t ".del"), admin_product_path(product), method: :delete %>
  </td>
  <td>
    <%= link_to (t ".edit"), edit_admin_product_path(product) %>
  </td>
</tr>
